{
  "name": "vscode-favourite",
  "displayName": "%ext.displayName%",
  "description": "%ext.description%",
  "version": "3.2.4",
  "publisher": "yunan-hu",
  "engines": {
    "vscode": "^1.68.0"
  },
  "scripts": {
    "watch": "tsc -watch -p ./",
    "build": "gulp build",
    "pub": "gulp publish",
    "clean": "gulp clean",
    "pk": "gulp package"
  },
  "keywords": [
    "favourite",
    "add to favourite",
    "favorite",
    "add to favorite",
    "add to favorites",
    "favorites",
    "收藏",
    "收藏夹",
    "添加到收藏夹"
  ],
  "categories": [
    "Other"
  ],
  "icon": "img/favourite.png",
  "bugs": {
    "url": "https://github.com/huyunan/vscode-favourite/issues",
    "email": "18842920976@163.com"
  },
  "homepage": "https://github.com/huyunan/vscode-favourite/blob/master/README.md",
  "repository": {
    "type": "git",
    "url": "https://github.com/huyunan/vscode-favourite.git"
  },
  "activationEvents": [
    "onCommand:favourite.addToFavourite",
    "onView:favourite-dir-view",
    "onView:favourite-mark-view",
    "onStartupFinished"
  ],
  "license": "GPL-3.0",
  "main": "./out/index.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "%ext.contributes.title%",
      "properties": {
        "favourite.resources": {
          "type": "array",
          "default": [],
          "description": "%ext.resources.description%"
        },
        "favourite.bookmarks": {
          "type": "array",
          "default": [],
          "description": "%ext.bookmarks.description%"
        },
        "favourite.sortOrder": {
          "type": "string",
          "enum": [
            "ASC",
            "DESC",
            "MANUAL"
          ],
          "default": "ASC",
          "description": "%ext.sortOrder.description%"
        },
        "favourite.currentGroup": {
          "type": "string",
          "default": null,
          "description": "%ext.currentGroup.description%"
        },
        "favourite.groups": {
          "type": "array",
          "default": [],
          "description": "%ext.groups.description%"
        }
      }
    },
    "viewsContainers": {
      "activitybar": [
        {
          "id": "favourite-explorer",
          "title": "%view.favourite.title%",
          "icon": "img/favourite.png"
        }
      ]
    },
    "views": {
      "favourite-explorer": [
        {
          "id": "favourite-dir-view",
          "name": "%view.favourite.title%",
          "when": "!ext:favourite-mark-view"
        },
        {
          "id": "favourite-mark-view",
          "name": "%view.favourite.title%",
          "when": "ext:favourite-mark-view"
        }
      ]
    },
    "menus": {
      "editor/context": [
        {
          "command": "favourite.addToFavourite",
          "when": "resourcePath not in ext:favorite.filePaths",
          "group": "5_favourite"
        },
        {
          "command": "favourite.deleteFavourite",
          "when": "resourcePath in ext:favorite.filePaths",
          "group": "5_favourite"
        }
      ],
      "editor/lineNumber/context": [
        {
          "command": "favourite.addToBookmark",
          "when": "editorLineNumber not in ext:favorite.markLineNumber",
          "group": "5_favourite"
        },
        {
          "command": "favourite.addToNameBookmark",
          "when": "editorLineNumber not in ext:favorite.markLineNumber",
          "group": "5_favourite"
        },
        {
          "command": "favourite.deleteBookmark",
          "when": "resourcePath in ext:favorite.markFilePaths && editorLineNumber in ext:favorite.markLineNumber",
          "group": "5_favourite"
        }
      ], 
      "explorer/context": [
        {
          "command": "favourite.addToFavourite",
          "when": "resourcePath not in ext:favorite.filePaths",
          "group": "5_favourite"
        },
        {
          "command": "favourite.deleteFavourite",
          "when": "resourcePath in ext:favorite.filePaths",
          "group": "5_favourite"
        }
      ],
      "view/title": [
        {
          "command": "favourite.file.reveal",
          "when": "view in ext:allFavouriteViews && !ext:favourite-mark-view",
          "group": "navigation@1"
        },
        {
          "command": "favourite.nav.refresh",
          "when": "view in ext:allFavouriteViews",
          "group": "navigation@4"
        },
        {
          "command": "favourite.nav.toggleView",
          "when": "view in ext:allFavouriteViews",
          "group": "navigation@5"
        },
        {
          "command": "favourite.group.changeGroup",
          "when": "view in ext:allFavouriteViews",
          "group": "navigation@2"
        },
        {
          "command": "favourite.group.newGroup",
          "when": "view in ext:allFavouriteViews",
          "group": "navigation@3"
        }
      ],
      "view/item/context": [
        {
          "command": "favourite.revealInOS.mac",
          "when": "isMac && (view in ext:allFavouriteViews && viewItem =~ /^resource/ || ext:favourite-mark-view)",
          "group": "navigation"
        },
        {
          "command": "favourite.revealInOS.windows",
          "when": "isWindows && (view in ext:allFavouriteViews && viewItem =~ /^resource/ || ext:favourite-mark-view)",
          "group": "navigation"
        },
        {
          "command": "favourite.revealInOS.other",
          "when": "!isMac && !isWindows && (view in ext:allFavouriteViews && viewItem =~ /^resource/ || ext:favourite-mark-view)",
          "group": "navigation"
        },
        {
          "command": "favourite.revealInSideBar",
          "when": "view in ext:allFavouriteViews || ext:favourite-mark-view",
          "group": "navigation"
        },
        {
          "command": "favourite.openToSide",
          "when": "(view in ext:allFavouriteViews && viewItem =~ /^resource(Child)?$|^uri\\.resource(Child)?$/ || ext:favourite-mark-view)",
          "group": "navigation"
        },
        {
          "command": "favourite.moveToTop",
          "when": "(view in ext:allFavouriteViews && viewItem =~ /resource(\\.dir)?$/ || ext:favourite-mark-view)",
          "group": "sort@1"
        },
        {
          "command": "favourite.moveUp",
          "when": "(view in ext:allFavouriteViews && viewItem =~ /resource(\\.dir)?$/ || ext:favourite-mark-view)",
          "group": "sort@2"
        },
        {
          "command": "favourite.moveDown",
          "when": "(view in ext:allFavouriteViews && viewItem =~ /resource(\\.dir)?$/ || ext:favourite-mark-view)",
          "group": "sort@3"
        },
        {
          "command": "favourite.moveToBottom",
          "when": "(view in ext:allFavouriteViews && viewItem =~ /resource(\\.dir)?$/ || ext:favourite-mark-view)",
          "group": "sort@4"
        },
        {
          "command": "favourite.sort",
          "when": "view in ext:allFavouriteViews && !ext:favourite-mark-view",
          "group": "sort@5"
        },
        {
          "command": "favourite.deleteFavourite",
          "when": "view in ext:allFavouriteViews && viewItem =~ /resource(\\.dir)?$/",
          "group": "inline@1"
        },
        {
          "command": "favourite.editBookmark",
          "when": "view in ext:allFavouriteViews && ext:favourite-mark-view",
          "group": "inline@1"
        },
        {
          "command": "favourite.deleteBookmark",
          "when": "view in ext:allFavouriteViews && ext:favourite-mark-view",
          "group": "inline@2"
        },
        {
          "command": "favourite.copyFilePath",
          "when": "view in ext:allFavouriteViews && !ext:favourite-mark-view",
          "group": "t-copy@1"
        },
        {
          "command": "favourite.copyRelativeFilePath",
          "when": "view in ext:allFavouriteViews && !ext:favourite-mark-view",
          "group": "t-copy@2"
        },
        {
          "command": "favourite.deleteAllFavourite",
          "when": "view in ext:allFavouriteViews",
          "group": "zbottom"
        }
      ],
      "commandPalette": [
        {
          "command": "favourite.nav.refresh",
          "when": "false"
        },
        {
          "command": "favourite.nav.toggleView",
          "when": "false"
        },
        {
          "command": "favourite.revealInOS.mac",
          "when": "false"
        },
        {
          "command": "favourite.revealInOS.windows",
          "when": "false"
        },
        {
          "command": "favourite.revealInOS.other",
          "when": "false"
        },
        {
          "command": "favourite.revealInSideBar",
          "when": "false"
        },
        {
          "command": "favourite.openToSide",
          "when": "false"
        },
        {
          "command": "favourite.moveToTop",
          "when": "false"
        },
        {
          "command": "favourite.moveUp",
          "when": "false"
        },
        {
          "command": "favourite.moveDown",
          "when": "false"
        },
        {
          "command": "favourite.copyFilePath",
          "when": "false"
        },
        {
          "command": "favourite.copyRelativeFilePath",
          "when": "false"
        },
        {
          "command": "favourite.moveToBottom",
          "when": "false"
        },
        {
          "command": "favourite.sort",
          "when": "false"
        }
      ]
    },
    "commands": [
      {
        "command": "favourite.file.reveal",
        "title": "%command.favourite.file.reveal%",
        "icon": {
          "dark": "img/reveal_black.svg",
          "light": "img/reveal_light.svg"
        }
      },
      {
        "command": "favourite.nav.refresh",
        "title": "%command.favourite.refresh%",
        "icon": {
          "dark": "img/refresh_black.svg",
          "light": "img/refresh_light.svg"
        }
      },
      {
        "command": "favourite.nav.toggleView",
        "title": "%command.favourite.toggleView%",
        "icon": {
          "dark": "img/toggle_view_black.svg",
          "light": "img/toggle_view_light.svg"
        }
      },
      {
        "command": "favourite.group.changeGroup",
        "title": "%command.favourite.changeGroup%",
        "icon": {
          "dark": "img/switch_group_black.svg",
          "light": "img/switch_group_light.svg"
        }
      },
      {
        "command": "favourite.group.newGroup",
        "title": "%command.favourite.newGroup%",
        "icon": {
          "dark": "img/new_group_black.svg",
          "light": "img/new_group_light.svg"
        }
      },
      {
        "command": "favourite.addToFavourite",
        "title": "%command.favourite.addtofavourite%"
      },
      {
        "command": "favourite.addToBookmark",
        "title": "%command.favourite.addToBookmark%"
      },
      {
        "command": "favourite.addToNameBookmark",
        "title": "%command.favourite.addToNameBookmark%"
      },
      {
        "command": "favourite.editBookmark",
        "title": "%command.favourite.editBookmark%",
        "icon": {
          "dark": "img/mark_edit_black.svg",
          "light": "img/mark_edit_light.svg"
        }
      },
      {
        "command": "favourite.deleteBookmark",
        "title": "%command.favourite.deleteBookmark%",
        "icon": {
          "dark": "img/item_del_black.svg",
          "light": "img/item_del_light.svg"
        }
      },
      {
        "command": "favourite.deleteFavourite",
        "title": "%command.favourite.deletefavourite%",
        "icon": {
          "dark": "img/item_del_black.svg",
          "light": "img/item_del_light.svg"
        }
      },
      {
        "command": "favourite.moveUp",
        "title": "%command.favourite.moveup%"
      },
      {
        "command": "favourite.moveDown",
        "title": "%command.favourite.movedown%"
      },
      {
        "command": "favourite.moveToTop",
        "title": "%command.favourite.moveToTop%"
      },
      {
        "command": "favourite.moveToBottom",
        "title": "%command.favourite.moveToBottom%"
      },
      {
        "command": "favourite.sort",
        "title": "%command.favourite.sort%"
      },
      {
        "command": "favourite.copyFilePath",
        "title": "%command.favourite.copyFilePath%"
      },
      {
        "command": "favourite.copyRelativeFilePath",
        "title": "%command.favourite.copyRelativeFilePath%"
      },
      {
        "command": "favourite.deleteAllFavourite",
        "title": "%command.favourite.deleteAllFavourite%"
      },
      {
        "command": "favourite.revealInOS.mac",
        "title": "%command.favourite.revealInOS.mac%"
      },
      {
        "command": "favourite.revealInOS.windows",
        "title": "%command.favourite.revealInOS.windows%"
      },
      {
        "command": "favourite.revealInOS.other",
        "title": "%command.favourite.revealInOS.other%"
      },
      {
        "command": "favourite.revealInSideBar",
        "title": "%command.favourite.revealInSideBar%"
      },
      {
        "command": "favourite.openToSide",
        "title": "%command.favourite.openToSide%"
      }
    ]
  },
  "devDependencies": {
    "@types/node": "^18.11.17",
    "@types/vscode": "^1.68.0",
    "@vscode/vsce": "^2.16.0",
    "del": "^7.0.0",
    "event-stream": "^4.0.1",
    "gulp": "^4.0.2",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "run-sequence": "^2.2.1",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "nconf": "^0.12.0"
  }
}
